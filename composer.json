{
  "name": "interactivesolutions/honeycomb-acl",
  "description": "HoneyComb access control list (ACL) package for users permissions and authentication.",
  "keywords": [
    "honeycomb",
    "laravel",
    "package",
    "acl",
    "access",
    "control",
    "control list",
    "users",
    "permissions",
    "authentication"
  ],
  "homepage": "https://github.com/interactivesolutions/honeycomb-acl/",
  "license": "MIT",
  "authors": [
    {
      "name": "Jevgenij Dmitrijev",
      "email": "jd@interactivesolutions.lt"
    },
    {
      "name": "Vytautas Rimeikis",
      "email": "vytautas.rimeikis@gmail.com"
    }
  ],
  "repositories": [],
  "require": {
    "php": ">=7.1",
    "laravel/framework": "^5.5",
    "guzzlehttp/guzzle": "^6.3",
    "interactivesolutions/honeycomb-core": "~1.1.0",
    "interactivesolutions/honeycomb-scripts": "~1.1.0",
    "interactivesolutions/honeycomb-languages": "~1.1.0"
  },
  "require-dev": {
    "brianium/paratest": "^1.0",
    "mockery/mockery": "^1.0",
    "orchestra/testbench-browser-kit": "^3.5",
    "phpunit/phpunit": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "InteractiveSolutions\\HoneycombAcl\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": [
      "paratest -p4 --colors"
    ]
  },
  "config": {
    "secure-http": false,
    "platform": {
      "php": "7.1"
    }
  }
}
